{
  "rules": {
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "curly": true,
    "class-name": true,
    "indent": [
      true,
      "spaces"
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": true
  },
  "defaultSeverity": "warning"
}
